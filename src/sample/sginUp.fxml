    <children>
        <Pane prefHeight="328.0" prefWidth="591.0" style="-fx-background-color: grey;">
            <children>
                <Text fill="#0a1581" layoutX="219.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Авторизация">
                    <font>
                        <Font size="27.0" />
                    </font>
                </Text>
                <Button fx:id="signUpButton" layoutX="243.0" layoutY="239.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="101.0" text="Авторизация" />
                <TextField fx:id="email" layoutX="225.0" layoutY="92.0" promptText="Email" />
                <TextField fx:id="password" layoutX="225.0" layoutY="139.0" promptText="Password" />
                <TextField fx:id="phone" layoutX="225.0" layoutY="182.0" promptText="Телефон" />
